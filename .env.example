OPENROUTER_API_KEY=your-openrouter-key

# Model to use for ChatOpenAI
OPENAI_MODEL=nvidia/nemotron-3-nano-30b-a3b:free

# OpenRouter API base URL (default points to openrouter.ai)
OPENAI_API_BASE=https://openrouter.ai/api/v1

# Logging Configuration
# LOG_LEVEL=INFO
# LOG_DIR=logs

# LangSmith Configuration (for LLM tracing and monitoring)
# Get your API key from: https://smith.langchain.com
# LANGSMITH_API_KEY=your-langsmith-api-key
# LANGSMITH_PROJECT=dev-team
# LANGSMITH_ENDPOINT=https://api.smith.langchain.com
# LANGCHAIN_TRACING_V2=true

